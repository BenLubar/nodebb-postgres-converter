CREATE TABLE "imported_discourse_messages" (
	"discourse_message_id" bigint NOT NULL,
	"nodebb_message_id" bigint NOT NULL
);

INSERT INTO "imported_discourse_messages" SELECT
       i."value"::bigint "discourse_message_id",
       i."score"::bigint "nodebb_message_id"
  FROM "objects_legacy" i
 WHERE i."key0" = '_imported'
   AND i."key1" = ARRAY['_messages'];

ALTER TABLE "imported_discourse_messages" ADD PRIMARY KEY ("discourse_message_id");

ALTER TABLE "imported_discourse_messages"
      CLUSTER ON "imported_discourse_messages_pkey";
