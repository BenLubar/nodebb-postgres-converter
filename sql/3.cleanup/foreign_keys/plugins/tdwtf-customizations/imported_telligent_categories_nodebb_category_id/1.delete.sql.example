DELETE FROM "imported_telligent_categories"
 USING "imported_telligent_categories" t1
  LEFT OUTER JOIN "categories" t2
    ON t1."nodebb_category_id" = t2."cid"
 WHERE t1."telligent_category_id" = "imported_telligent_categories"."telligent_category_id"
   AND t1."nodebb_category_id" IS NOT NULL
   AND t2."cid" IS NULL;
